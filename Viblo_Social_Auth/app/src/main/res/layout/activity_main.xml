<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="10dp"
    tools:context="com.framgia.viblo_social_auth.MainActivity">
    <Button
        android:id="@+id/btn_authencation"
        style="@style/viewCustom"
        android:layout_below="@id/btn_database"
        android:background="@color/colorAccent"
        android:text="@string/btn_authen"
        android:textColor="@android:color/white"
        android:textStyle="bold" />
    <Button
        android:id="@+id/btn_database"
        style="@style/viewCustom"
        android:layout_below="@id/btn_ga"
        android:background="@color/colorAccent"
        android:text="@string/btn_database"
        android:textColor="@android:color/white"
        android:textStyle="bold" />
    <Button
        android:id="@+id/btn_ga"
        style="@style/viewCustom"
        android:background="@color/colorAccent"
        android:text="@string/btn_ga"
        android:textColor="@android:color/white"
        android:textStyle="bold" />

</RelativeLayout>
